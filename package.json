{
  "name": "nebenan-react-hocs",
  "description": "Collection of React High Order Components",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-react-hocs",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-react-hocs/issues",
  "version": "1.0.0",
  "engines": {
    "node": "^8.x.x"
  },
  "scripts": {
    "clean": "rm -rf ./lib/ || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run clean && npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run build:babel && env TZ='Europe/Berlin' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build:babel && mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-node-env-inline": "0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "5.2.0",
    "nebenan-helpers": "^2.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-redux": "^5.0.7",
    "date-fns": "^1.x",
    "redux": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^16.x.x",
    "prop-types": "^15.x.x",
    "nebenan-helpers": "^2.0.0",
    "react-redux": "^5.0.7",
    "date-fns": "^1.x",
    "redux": "^4.0.0"
  },
  "dependencies": {
    "node-polyglot": "^2.3.0"
  }
}
