{
  "name": "nebenan-react-hocs",
  "description": "Collection of React High Order Components",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-react-hocs",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-react-hocs/issues",
  "version": "3.0.0-beta.0",
  "engines": {
    "node": "^10.x.x"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run check && npm run clean && npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run build:babel && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build:babel && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "date-fns": "^1.30.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "mocha": "^6.1.4",
    "nebenan-helpers": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.4.5",
    "react": "^16.8.6",
    "prop-types": "^15.7.2",
    "nebenan-helpers": "^3.2.0",
    "react-redux": "^7.1.0",
    "date-fns": "^1.30.1",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "node-polyglot": "^2.3.1"
  }
}
