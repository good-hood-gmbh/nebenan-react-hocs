{
  "name": "nebenan-react-hocs",
  "description": "Collection of React High Order Components",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-react-hocs",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-react-hocs/issues",
  "version": "5.3.0-beta.0",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run compile && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "date-fns": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-config-nebenan": "^1.0.2",
    "mocha": "^6.2.2",
    "nebenan-helpers": "^4.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "react": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "redux": "^4.0.4",
    "terser": "^4.3.9"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.6.2",
    "react": "^16.9.0",
    "prop-types": "^15.7.2",
    "nebenan-helpers": "^3.3.0 || ^4.0.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1"
  },
  "dependencies": {
    "node-polyglot": "^2.4.0"
  }
}
